{
  "name": "@spark-web/docs",
  "version": "1.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --sequential build:*",
    "build:contentlayer": "INIT_CWD=$PWD yarn contentlayer build",
    "build:docs": "yarn generate-design-system-package && INIT_CWD=$PWD next build",
    "build:playroom": "yarn generate-design-system-package && playroom build",
    "build:storybook": "build-storybook -o ./public/storybook",
    "check:types": "yarn generate-design-system-package && yarn build:contentlayer && tsc --noEmit",
    "dev:docs": "yarn generate-design-system-package && INIT_CWD=$PWD next dev",
    "dev:playroom": "playroom start",
    "dev:storybook": "start-storybook -p 6006",
    "generate-design-system-package": "../scripts/generate-design-system-package.mjs",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "@emotion/css": "^11.9.0",
    "@spark-web/design-system": "^0.0.0",
    "@untitled-docs/live-code": "^0.4.0",
    "clipboard-copy": "^4.0.1",
    "lunr": "^2.3.9",
    "next": "^12.1.6",
    "next-seo": "^5.4.0",
    "parse-numeric-range": "^1.3.0",
    "prism-react-renderer": "^1.3.3",
    "react": "^17.0.2",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-simple-code-editor": "^0.11.2"
  },
  "devDependencies": {
    "@preconstruct/next": "^4.0.0",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.5.9",
    "@types/node": "16.11.13",
    "@types/react": "^17.0.12",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "chokidar": "^3.5.3",
    "contentlayer": "^0.2.5",
    "date-fns": "^2.28.0",
    "next-compose-plugins": "^2.2.1",
    "next-contentlayer": "^0.2.5",
    "playroom": "^0.28.0",
    "postcss": "^8.4.14",
    "rehype-slug": "^5.0.1",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-mdx": "^2.1.2",
    "strip-markdown": "^5.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0"
  }
}
